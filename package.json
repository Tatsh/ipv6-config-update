{
  "contributors": [
    "Andrew Udvare <audvare@gmail.com>"
  ],
  "devDependencies": {
    "cspell": "^6.31.1",
    "prettier": "^2.8.8",
    "prettier-plugin-ini": "^1.1.0",
    "prettier-plugin-toml": "^0.3.1"
  },
  "license": "MIT",
  "name": "ipwatch",
  "repository": "git@github.com:Tatsh/ipwatch.git",
  "scripts": {
    "check-formatting": "prettier -c './**/*.md' './**/*.yml' ./.prettierrc",
    "check-spelling": "cspell --no-progress './**/*'  './**/.*'",
    "format": "prettier -w './**/*.md' './**/*.yml' ./.prettierrc",
    "qa": "yarn check-spelling && yarn check-formatting"
  },
  "version": "0.0.1"
}
